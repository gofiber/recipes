{
  "name": "template-asset-bundling",
  "scripts": {
    "predev": "npm run clean",
    "dev": "parcel watch --dist-dir ./public/assets/ assets/**/*",
    "prebuild": "npm run clean",
    "build": "parcel build --no-source-maps --dist-dir ./public/assets/ assets/**/*",
    "clean": "rm -rf ./public/assets"
  },
  "devDependencies": {
    "parcel": "2.16.4",
    "postcss": "8.5.6",
    "tailwindcss": "4.1.18"
  }
}